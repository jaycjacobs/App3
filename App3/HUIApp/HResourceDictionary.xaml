    <SolidColorBrush x:Key="SliderThumbPressedBackgroundThemeBrush">Gainsboro</SolidColorBrush>
    <SolidColorBrush x:Key="SliderThumbPressedBorderThemeBrush">DarkGray</SolidColorBrush>

